{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "member-access": [true, "no-public"],
    "interface-name": false,
    "object-literal-sort-keys": false,
    "space-before-function-paren": true,
    "ordered-imports": false,
    "no-console": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules/*",
      "dist/*",
      "webpack/info/*",
      "coverage/*",
      ".temp/*",
      ".idea/*",
      ".vscode/*"
    ]
  }
}
